<launch>

    <master auto="start"/>
 	<!-- Run the map server --> 
    
 	<!--- Run AMCL --> 
    <include file="$(find amcl)/examples/amcl_omni.launch" />

    <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	    <rosparam file="$(find redrover_2dnav)/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
	    <rosparam file="$(find redrover_2dnav)/costmap_common_params.yaml" command="load" ns="local_costmap" />
	    <rosparam file="$(find redrover_2dnav)/local_costmap_params.yaml" command="load" />
	    <rosparam file="$(find redrover_2dnav)/global_costmap_params.yaml" command="load" /> 
	    <rosparam file="$(find redrover_2dnav)/base_local_planner_params.yaml" command="load" />
 	</node>
</launch> 
